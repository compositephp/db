{
    "name": "compositephp/db",
    "description": "PHP 8.1+ ORM and DataMapper",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Composite PHP",
            "email": "compositephp@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/simple-cache":  "1 - 3",
        "cycle/database": "^2.2.1",
        "cycle/migrations": "^v3.0.0",
        "iamcal/sql-parser": "^0.4.0",
        "symfony/console": "2 - 6"
    },
    "require-dev": {
        "kodus/file-cache": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Composite\\DB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Composite\\DB\\Tests\\": "tests/"
        }
    }
}
